     ]
    }
   ],
   "source": [
    "def primeFunction():\n",
    "    prime=None\n",
    "    num = 1\n",
    "    while True:\n",
    "        num = num + 1\n",
    "  \n",
    "        for i in range(2, num): \n",
    "            if(num % i) == 0:\n",
    "                prime=False\n",
    "                break\n",
    "            else:\n",
    "                prime = True\n",
    "  \n",
    "        if prime:\n",
    "              yield num\n",
    "  \n",
    "def main():\n",
    "      prime = primeFunction()\n",
    "      for i in prime:\n",
    "        print(i)\n",
    "        if i > 50:\n",
    "            break\n",
    "  \n",
    "if __name__ == \"__main__\":\n",
    "    main()\n",
    "\n",
    "\n"
   ]
